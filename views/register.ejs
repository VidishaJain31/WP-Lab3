<%- include('header'); -%> 
    <div class="Register"> 
        <h1>Register to Webstore</h1>
        <form action="/api/register" method="post">
            <br>
            <label>UserName : </label>
            <input type="text" name="username" required><br><br>
            <label>Password : </label>
            <input type="password" name="password" required><br><br>
            <label>Society : </label>
            <input type="text" name="society" required><br><br>
            <label>Contact : </label>
            <input type="text" name="contact" required><br><br>
            <label>Address : </label>
            <input type="text" name="addres" required><br><br>
            <label>ZipCode : </label>
            <input type="text" name="zipcode" required><br><br>
            <label>City : </label>
            <input type="text" name="city" required><br><br>
            <label>Phone : </label>
            <input type="number" name="phone" required><br><br>
            <label>FAX : </label>
            <input type="text" name="fax" required><br><br>
            <label>Max-outstanding : </label>
            <input type="text" name="max_outstanding" required><br><br>
            <button type="submit">Register to Webstore</button><br>
        </form>
    </div>
<%- include('footer'); -%>